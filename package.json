{
  "name": "raagapella",
  "version": "0.2.0",
  "description": "Website for Stanford Raagapella, Stanford's South Asian A Cappella Group",
  "private": "true",
  "main": "server.js",
  "engines": {
    "node": "8.1.4",
    "npm": "5.2.0"
  },
  "scripts": {
    "express-server": "./node_modules/.bin/babel-node server/server.js",
    "express-dev-server": "./node_modules/.bin/babel-watch server/server.js",
    "webpack": "./node_modules/.bin/webpack --colors",
    "webpack-watch": "./node_modules/.bin/webpack -w --colors",
    "dev": "npm run webpack-watch & npm run express-dev-server",
    "build": "npm run webpack",
    "start": "npm run express-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit-msg": "echo 'pre-commit frontend webpack build...' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "build"
  ],
  "dependencies": {
    "airtable": "^0.4.5",
    "axios": "^0.12.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.3",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "ics": "^0.5.0",
    "mailgun-js": "^0.9.1",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "mongoose": "^4.13.17",
    "pre-commit": "^1.2.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-router": "^2.8.1",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "babel-loader": "^9.1.0",
    "babel-watch": "^2.0.7",
    "dotenv": "^2.0.0",
    "json-loader": "^0.5.7",
    "react-bootstrap": "^0.30.10",
    "react-select": "^1.3.0",
    "webpack": "^5.74.0"
  }
}
